# Neo4j 数据库配置
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=
NEO4J_DATABASE=neo4j

# LLM 配置（通义千问）
# 北京地域（大陆访问）
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# 新加坡地域（国际访问）
# LLM_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1

LLM_API_KEY=
LLM_MODEL=qwen-plus

# LLM 模型选择（两阶段处理）
# 快速模型：用于生成查询计划（qwen-flash 速度快）
LLM_PLAN_MODEL=qwen-flash
# 智能模型：用于生成答案（qwen-plus 质量高）
LLM_ANSWER_MODEL=qwen-plus

# NLQ 限制配置
NLQ_MAX_ROWS=50
NLQ_MAX_NODES=80
NLQ_MAX_EDGES=120

# Next.js 环境
NODE_ENV=development

